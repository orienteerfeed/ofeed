{
  "branches": ["main"],
  "tagFormat": "v${version}",
  "plugins": [
    [
      "@semantic-release/exec",
      {
        "analyzeCommitsCmd": "echo patch",
        "generateNotesCmd": "echo \"Automated release generated from main.\""
      }
    ],
    [
      "@semantic-release/github",
      {
        "successComment": false,
        "failComment": false
      }
    ]
  ]
}
